# React projesini oluştur
npx create-react-app .

# Gerekli paketleri yükle
npm install react-router-dom axios @reduxjs/toolkit react-redux styled-components

# Proje klasörüne gir
cd easyskins-clone

# Git'i başlat
git init

# Dosyaları ekle
git add .

# Commit yap
git commit -m "ilk commit"

# GitHub reposunu bağla
git remote add origin https://github.com/ismailuzunn/easyskins-clone.git

# Branch'i ayarla
git branch -M main

# GitHub'a gönder
git push -u origin main

# Build alın
npm run build

# build klasörünü FTP ile hostinginize yükleyin 

pwd

# Klasör içeriğini listeler
ls

git status 

# Projenizin klasörüne git
cd C:\Users\user\Yeni klasör

# Git'i başlat
git init

# Dosyaları ekle
git add .

# Commit yap
git commit -m "ilk commit"

# GitHub reposunu bağla
git remote add origin https://github.com/ismailuzunn/easyskins-clone.git

# Branch'i ayarla
git branch -M main

# GitHub'a gönder
git push -u origin main 